{
  "cli": {
    "version": ">= 16.4.0",
    "appVersionSource": "remote",
    "requireCommit": true
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "development",
      "releaseChannel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "releaseChannel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "releaseChannel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
